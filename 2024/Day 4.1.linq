<Query Kind="Program" />

void Main()
{
	var matches = 0;

	var lines = _input.Split("\r\n");
	for (int i = 0; i < lines.Length; i++)
	{
		var line = lines[i];
		for (int j = 0; j < line.Length; j++)
		{
			if (line[j] == 'X')
			{
				var checkWest = j >= 3;
				var checkEast = j < line.Length - 3;
				var checkNorth = i >= 3;
				var checkSouth = i < lines.Length - 3;
				var checkNorthWest = checkNorth && checkWest;
				var checkNorthEast = checkNorth && checkEast;
				var checkSouthWest = checkSouth && checkWest;
				var checkSouthEast = checkSouth && checkEast;
				
				if (checkWest && line[j - 1] == 'M' && line[j - 2] == 'A' && line[j - 3] == 'S') matches++;
				if (checkEast && line[j + 1] == 'M' && line[j + 2] == 'A' && line[j + 3] == 'S') matches++;
				if (checkNorth && lines[i - 1][j] == 'M' && lines[i - 2][j] == 'A' && lines[i - 3][j] == 'S') matches++;
				if (checkSouth && lines[i + 1][j] == 'M' && lines[i + 2][j] == 'A' && lines[i + 3][j] == 'S') matches++;
				if (checkNorthWest && lines[i - 1][j - 1] == 'M' && lines[i - 2][j - 2] == 'A' && lines[i - 3][j - 3] == 'S') matches++;
				if (checkNorthEast && lines[i - 1][j + 1] == 'M' && lines[i - 2][j + 2] == 'A' && lines[i - 3][j + 3] == 'S') matches++;
				if (checkSouthWest && lines[i + 1][j - 1] == 'M' && lines[i + 2][j - 2] == 'A' && lines[i + 3][j - 3] == 'S') matches++;
				if (checkSouthEast && lines[i + 1][j + 1] == 'M' && lines[i + 2][j + 2] == 'A' && lines[i + 3][j + 3] == 'S') matches++;
			}
		}
	}
	
	matches.Dump("Number of XMAS matches");
}

