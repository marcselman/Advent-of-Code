<Query Kind="Program" />

void Main()
{
	var totalMatches = 0;

	var lines = _input.Split("\r\n");
	for (int i = 0; i < lines.Length; i++)
	{
		var line = lines[i];
		for (int j = 0; j < line.Length; j++)
		{
			if (line[j] == 'A')
			{
				var matches = 0;
				
				var checkDiagonal1 = j >= 1 && j < line.Length - 1 && i >= 1 && i < lines.Length - 1;

				if (checkDiagonal1 && lines[i - 1][j - 1] == 'M' && lines[i + 1][j + 1] == 'S') matches++;
				if (checkDiagonal1 && lines[i - 1][j - 1] == 'S' && lines[i + 1][j + 1] == 'M') matches++;

				if (checkDiagonal1 && lines[i - 1][j + 1] == 'M' && lines[i + 1][j - 1] == 'S') matches++;
				if (checkDiagonal1 && lines[i - 1][j + 1] == 'S' && lines[i + 1][j - 1] == 'M') matches++;
				
				if (matches == 2) totalMatches++;
			}
		}
	}
	
	totalMatches.Dump("Number of XMAS matches");
}

